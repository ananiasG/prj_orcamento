      '  ((PRODUTO.produtoid IS NULL AND :OLD_produtoid IS NULL) OR (PRODUTO.produtoid = :OLD_produtoid))'
    )
    InsertSQL.Strings = (
      'INSERT INTO PRODUTO'
      '  (produtoid, ds_produto, obs_produto, vl_venda_produto, dt_cadastro_produto, '
      '   status_produto, ds_categoria_produto)'
      'VALUES'
      '  (:produtoid, :ds_produto, :obs_produto, :vl_venda_produto, :dt_cadastro_produto, '
      '   :status_produto, :ds_categoria_produto)'
    )
    ModifySQL.Strings = (
